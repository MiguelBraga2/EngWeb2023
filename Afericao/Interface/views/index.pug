extends layout
block content
  .w3-card-4
    .w3-container
      table.w3-table-all
        tr
          th data
          th Nome
          th Género
          th Idade 
          th Resultado        
        each p in list
          tr
              td 
                a(href="/" + p._id) p.dataEMD
              td #{p.nome.primeiro} #{p.nome.último}
              td= p.género
              td= p.idade
              td= p.resultado
              
    footer.w3-container.w3-teal
      h5 Generated by EMD::EngWeb2023 in #{d}
